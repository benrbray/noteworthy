    "@benrbray/remark-cite": "^1.1.0",
    "@citation-js/core": "^0.5.7",
    "@citation-js/date": "^0.5.1",
    "@citation-js/plugin-bibtex": "^0.5.7",
    "@citation-js/plugin-csl": "^0.5.7",
    "@codemirror/commands": "^6.3.3",
