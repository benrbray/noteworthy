    "@electron/asar": "^3.2.9",
    "@electron/notarize": "^1.2.4",
    "@types/electron-store": "^3.2.0",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.0",
    "@types/mdast": "^3.0.15",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.31",
    "@types/path-browserify": "^1.0.2",
    "@types/unist": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
